<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive authentication data from cloud backup -->
        <exclude domain="sharedpref" path="auth_prefs.xml"/>
        <exclude domain="sharedpref" path="stream_keys.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- Allow app data transfer between devices -->
        <include domain="sharedpref" path="."/>
        <include domain="file" path="."/>
    </device-transfer>
</data-extraction-rules>
